<h1>Listing admin_hotels</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Description</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_hotels.each do |admin_hotel| %>
  <tr>
    <td><%= admin_hotel.name %></td>
    <td><%= admin_hotel.address %></td>
    <td><%= admin_hotel.description %></td>
    <td><%= admin_hotel.latitude %></td>
    <td><%= admin_hotel.longitude %></td>
    <td><%= link_to 'Show', admin_hotel %></td>
    <td><%= link_to 'Edit', edit_admin_hotel_path(admin_hotel) %></td>
    <td><%= link_to 'Destroy', admin_hotel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Hotel', new_admin_hotel_path %>
